{
  "cSpell.words": [
    "-cacerts",
    "-clcerts",
    "-nocerts",
    "-nocrypt",
    "-nokeys",
    "-noout",
    "-passin",
    "-pubkey",
    "pkcs12",
    "pkcs8"
  ],
  "[jsonc]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  }
}
